generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_POSTGRE")
}

model Notification {
  id        String   @id @default(uuid())
  userId    String   // tá»« Auth
  type      String   // TASK, PROJECT, SYSTEM
  title     String
  message   String
  read      Boolean  @default(false)
  createdAt DateTime @default(now())
}
